@InCollection{Weiss1997,
  Title                    = {{ EEG-Koh\"arenz und Sprachverarbeitung: Die funktionelle Verkopplung von Gehirnregionen w\"ahrend der Verarbeitung unterschiedlicher Nomina}},
  Author                   = {Weiss, S.},
  Booktitle                = {{Studien zur Klinischen Linguistik: Methoden, Modelle, Intervention.}},
  Publisher                = {Opladen: Westdeutscher Verlag},
  Year                     = {1997},
  Editor                   = {G. Rickheit},
  Pages                    = {125-146},

  Abstract                 = {Mit Hilfe von Skalpelektroden kann die elektrische Aktivit\"at von Gehirnregionen w\"ahrend der Verarbeitung von Sprache gemessen werden (Elektroenzephalogramm, EEG). F\"ur die Sprachverarbeitung ist nicht die Aktivierung engumgrenzter, unabh\"angig agierender Gehirnregionen verantwortlich, sondern die parallele Kooperation mehrerer Regionen. Aus diesem Grund wurde in der hier beschriebenen Studie die Koh\"arenz zwischen EEG-Signalen berechnet, wobei eine hohe Koh\"arenz zwischen den EEG-Signalen zweier Gehirnregionen als Ausdruck einer gesteigerten funktionellen Kooperation dieser Regionen interpretiert wird. Es wurden EEG-Koh\"arenzen von 19 Versuchspersonen berechnet, die sich akustisch pr\"asentierte Konkreta, Abstrakta und Pseudow\"orter merken sollten. Alle Aufgaben zeigten \"ahnliche Koh\"arenzmuster im Alpha-1-Band (8-10 Hz), was als Ausdruck akustisch, lexikalischer Verarbeitung der W\"orter interpretiert wurde. Das Beta-1-Band (13-18 Hz) hingegen zeigte in Abh\"angigkeit der Wortart Koh\"arenzmuster, die mit assoziativen Prozessen und komplexen kognitiven Leistungen verbunden sind. Im Sinne der heutigen Netzwerktheorien k\"onnte die Koh\"arenzanalyse des EEG in Zukunft nicht nur zum Verst\"andnis der intakten Sprachverarbeitung, sondern auch zum Verst\"andnis des Verlaufs und der R\"uckbildung von Sprachst\"orungen beitragen.},
  Owner                    = {eva},
  Timestamp                = {2008.10.27}
}

